		for(int i =0;i<input.length();i++) {
			for(int j=i+1;i<input.length();i++) {
				if(input.charAt(i)==input.charAt(j)) {
					return false;
				}
			}
		}
		
		
		return true;
		
	}
	
	
	
	
	public static void main(String args[]) {
